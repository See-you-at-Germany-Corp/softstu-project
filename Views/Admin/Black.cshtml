@{
    var users = ViewData["Users"] as List<UserBlacklist>;
}

<div>
    <h3>Users</h3>
    @foreach (var user in users)
    {
        if (!user.is_blacklist)
        {
            <div>
                <img style="width: 50px; height: 50px" src="../../images/@(user.gender).png" alt="img">
                <p>@user.fname @user.lname</p>
            </div>
        }
    }
</div>

<div>
    <h3>Blacklists</h3>
    @foreach (var user in users)
    {
        if (user.is_blacklist)
        {
            <div>
                <img style="width: 50px; height: 50px" src="../../images/@(user.gender).png" alt="img">
                <p>@user.fname @user.lname</p>
            </div>
        }
    }
</div>