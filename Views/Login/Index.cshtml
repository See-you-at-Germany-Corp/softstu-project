@{
    ViewData["Title"] = "Login";
    int userID = (int)TempData["userID"];
}
<div class="contain-login">
    <div class="login">
        <div class="logo-lab">
            <img class="logo-pic"
                src="https://cdn.discordapp.com/attachments/817783466379968572/832547225749553172/flask.png" alt="dog">
            <div class="logo-text">
                <p class="font-header">Double Labs</p>
            </div>
        </div>
        <div class="login-input">
            <form asp-controller="Login" class="contain-input" asp-action="Submit">
                <input class="input" id="username" type="text" name ="username" placeholder="username">
                <input class="input" id="password" type="password" name ="password" placeholder="password">
                <button class="login-btn" id="login" type="submit">Login</button>
            </form>
        </div>
    </div>
</div>